{
  "name": "@xmtp/wa-sqlite",
  "version": "1.0.0",
  "type": "module",
  "main": "src/sqlite-api.js",
  "types": "src/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/types/index.d.ts",
      "default": "./src/sqlite-api.js"
    },
    "./build": {
      "types": "./src/types/index.d.ts",
      "default": "./dist/wa-sqlite.mjs"
    },
    "./build/asyncify": {
      "types": "./src/types/index.d.ts",
      "default": "./dist/wa-sqlite-async.mjs"
    },
    "./build/jspi": {
      "types": "./src/types/index.d.ts",
      "default": "./dist/wa-sqlite-jspi.mjs"
    },
    "./vfs/AccessHandlePool": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/AccessHandlePoolVFS.js"
    },
    "./vfs/IDBBatchAtomic": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/IDBBatchAtomicVFS.js"
    },
    "./vfs/IDBMirror": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/IDBMirrorVFS.js"
    },
    "./vfs/MemoryAsync": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/MemoryAsyncVFS.js"
    },
    "./vfs/Memory": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/MemoryVFS.js"
    },
    "./vfs/OPFSAdaptive": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/OPFSAdaptiveVFS.js"
    },
    "./vfs/OPFSAnyContext": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/OPFSAnyContextVFS.js"
    },
    "./vfs/OPFSCoopSync": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/OPFSCoopSyncVFS.js"
    },
    "./vfs/OPFSPermuted": {
      "types": "./src/types/index.d.ts",
      "default": "./src/examples/OPFSPermutedVFS.js"
    }
  },
  "files": [
    "src/sqlite-constants.js",
    "src/sqlite-api.js",
    "src/types/*",
    "src/FacadeVFS.js",
    "src/VFS.js",
    "src/WebLocksMixin.js",
    "src/examples/*",
    "dist/*",
    "test/*"
  ],
  "scripts": {
    "build-docs": "typedoc",
    "start": "web-dev-server --node-resolve",
    "test": "web-test-runner",
    "test-manual": "web-test-runner --manual"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@web/dev-server": "^0.4.6",
    "@web/test-runner": "^0.18.2",
    "@web/test-runner-core": "^0.13.3",
    "comlink": "^4.4.1",
    "jasmine-core": "^4.5.0",
    "monaco-editor": "^0.34.1",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "web-test-runner-jasmine": "^0.0.6"
  },
  "dependenciesMeta": {
    "monaco-editor@0.34.1": {
      "unplugged": true
    },
    "web-test-runner-jasmine@0.0.6": {
      "unplugged": true
    }
  },
  "packageManager": "yarn@4.0.2"
}
